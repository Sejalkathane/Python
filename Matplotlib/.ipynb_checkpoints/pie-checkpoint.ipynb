    "import matplotlib.pyplot as plt\n",
    "x=[57,23,76,72,11]\n",
    "y=[\"a\",\"b\",\"c\",\"d\",\"e\"]\n",
    "ex=[0.0,0.0,0.0,0.0,0.2]\n",
    "# using explode seperate small part\n",
    "c=[\"red\",\"blue\",\"green\",\"yellow\",\"m\"]\n",
    "plt.pie(x,labels=y,explode=ex,colors=c,autopct=\"%0.2f%%\",shadow=True)\n",
    "plt.show()\n"
   ]
